             <property name="styleSheet">
              <string notr="true">QPushButton {
    /* -- Semi-transparent black background -- */
    background-color: rgba(0, 0, 0, 150);
    
    /* -- Subtle gray border -- */
    border: 1px solid rgba(100, 255, 100, 60);  /* Slight green tint */
    border-radius: 12px;
    
    /* -- Text styling -- */
    color: white;
    font: bold 22px &quot;Helvetica&quot;;
    padding: 8px 16px;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 150);
    
    /* -- Soft inner reflection and slight outer shadow -- */
    box-shadow: 
        0 2px 4px rgba(0, 0, 0, 100),
        inset 0 1px 1px rgba(255, 255, 255, 20);

    transition: all 0.2s ease-in-out;
}

/* ===== HOVER STATE (Brighter green background + glow) ===== */
QPushButton:hover {
    background-color: rgba(0, 80, 0, 180);  /* Dark green translucent */
    border-color: rgba(0, 255, 100, 180);
    box-shadow: 0 0 12px rgba(0, 255, 100, 200);
}

/* ===== PRESSED STATE (Even stronger green) ===== */
QPushButton:pressed {
    background-color: rgba(0, 120, 0, 200);  /* Brighter green background */
    border-color: rgba(0, 255, 150, 255);
    box-shadow: 0 0 20px rgba(0, 255, 100, 255);
    padding-top: 9px;
    padding-bottom: 7px;
}

/* ===== ACCENT VARIATION (Optional) ===== */
QPushButton#accent {
    border-color: rgba(0, 255, 150, 220);
    box-shadow: 0 0 14px rgba(0, 255, 150, 180);
}

</string>
