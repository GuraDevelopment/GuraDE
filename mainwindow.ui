           <string notr="true">
QWidget {
    background-color: rgba(20, 20, 20, 180);  /* Main semi-transparent dark */
    color: #aafaff;
    font-family: &quot;Segoe UI&quot;, &quot;Noto Sans&quot;, sans-serif;
    font-size: 24px;
}

/* Calendar Container */
QCalendarWidget {
    background-color: transparent;
    border: 1px solid rgba(0, 255, 255, 50);
    border-radius: 8px;
    padding: 3px;
}

/* Top Navigation Bar */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: rgba(0, 255, 255, 25);
    border-bottom: 1px solid rgba(0, 255, 255, 60);
}

/* Navigation Buttons */
QCalendarWidget QToolButton {
    background-color: transparent;
    color: #aafaff;
    border: none;
    padding: 3px 6px;
    font-weight: bold;
}

QCalendarWidget QToolButton:hover {
    background-color: rgba(0, 255, 255, 40);
    border-radius: 4px;
}

/* Month/Year Dropdown */
QCalendarWidget QMenu {
       background-color: rgba(0, 255, 255, 25);
     /* FIX: previously solid black */
    color: #aafaff;;
    border: 1px solid rgba(0, 255, 255, 80);
}

/* Spinboxes (for year/month switching) */
QCalendarWidget QSpinBox {
    background-color: rgba(40, 40, 40, 160);
    color: #aafaff;
    border: 1px solid rgba(0, 255, 255, 50);
    border-radius: 4px;
    padding: 2px;
}

/* Grid of Days */
QCalendarWidget QAbstractItemView {
    background-color: rgba(0, 255, 255, 25);
     /* FIX: previously solid black */
    color: #aafaff;
    selection-background-color: rgba(0, 255, 255, 80);
    selection-color: black;
    gridline-color: rgba(170, 255, 255, 20);
}

/* Hover effect for individual days */
QCalendarWidget QAbstractItemView:item:hover {
    background-color: rgba(0, 255, 255, 30);
    color: white;
}

/* Selected day */
QCalendarWidget QAbstractItemView:item:selected {
    background-color: #00ffff;
    color: black;
    border-radius: 4px;
}

/* Today outline */
QCalendarWidget QAbstractItemView:item {
    border: 1px solid transparent;
}
QCalendarWidget QAbstractItemView:item:enabled {
    margin: 1px;
    padding: 2px;
    border: 3px solid rgba(0, 255, 255, 50);
}
QCalendarWidget QAbstractItemView:item:enabled:selected {
    font-weight: bold;
}





