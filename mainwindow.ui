              <string notr="true">/* ===== FIREFOX ORANGE BUTTON - BASE STYLE ===== */
QPushButton {
    /* -- Vibrant Orange Gradient -- */
    background-color: qradialgradient(
        spread:pad,
        cx:0.5, cy:0.5, radius:0.6,
        fx:0.5, fy:0.5,
        stop:0 rgba(255, 180, 80, 200),   /* Light orange */
        stop:0.5 rgba(230, 90, 30, 180),  /* Firefox orange */
        stop:1 rgba(190, 60, 0, 220)      /* Deep rust */
    );
    
    /* -- Glowing Border -- */
    border: 1px solid rgba(255, 160, 60, 220);
    border-radius: 12px;
    
    /* -- Text Styling -- */
    color: white;
    font: bold 22px &quot;Helvetica&quot;;
    padding: 8px 16px;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 40);
    
    /* -- 3D Effects -- */
    box-shadow: 
        0 2px 4px rgba(0, 0, 0, 80),
        inset 0 1px 1px rgba(255, 255, 255, 120);
}

/* ===== HOVER STATE (Fire Glow) ===== */
QPushButton:hover {
    background-color: qradialgradient(
        spread:reflect,
        cx:0.5, cy:0.5, radius:0.8,
        fx:0.5, fy:0.5,
        stop:0 rgba(255, 200, 100, 220),
        stop:0.7 rgba(240, 120, 50, 200),
        stop:1 rgba(210, 70, 10, 240)
    );
    border-color: rgba(255, 180, 80, 240);
}

/* ===== PRESSED STATE (Ember Effect) ===== */
QPushButton:pressed {
    background-color: qradialgradient(
        spread:pad,
        cx:0.5, cy:0.5, radius:0.3,
        fx:0.5, fy:0.5,
        stop:0 rgba(210, 100, 40, 220),
        stop:1 rgba(170, 50, 0, 240)
    );
    box-shadow: inset 0 2px 3px rgba(0, 0, 0, 100);
}

/* ===== FIREFOX-STYLE BADGE (Optional) ===== */
QPushButton#firefoxBadge {
    background-color: rgba(230, 90, 30, 220);
    border-radius: 6px;
    padding: 2px 6px;
    font: bold 9px;
    margin-left: 5px;
}</string>
